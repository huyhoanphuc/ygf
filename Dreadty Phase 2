function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local Souand = Instance.new("Sound")
    Souand.SoundId = GitAud(soundlink, filename)
    Souand.Parent = Workspace
    Souand.Looped = true
    Souand.Volume = 1.5
    Souand:Play()
end

CustomGitSound("https://github.com/huyhoanphuc/ygf/blob/main/videoplayback (10)_001.mp3?raw=true", 1, "DOORSRsjhhshgshgOSTTheFigureEnraged")

